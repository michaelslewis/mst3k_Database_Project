{{> header}}

<div class="container-fluid text-center">
  <h1>View or add data to the 'actor_character' table:</h1>
</div>

<div class="container-fluid col-md-offset-3">
  <form class="form-inline">
      <div class="row">
        <div class="form-group ">
          <input type="number" class="form-control" id="addactor_id" placeholder="actor_id">
        </div>
        <div class="form-group ">
          <input type="number" class="form-control" id="addcharacter_id" placeholder="character_id">
        </div>
        <div class="form-group edit-buttons">
          <button id="addRow" class="btn btn-default" role="button" type="button" name="button">Add Row</button>
        </div>
      </div>
  </form>
</div>

